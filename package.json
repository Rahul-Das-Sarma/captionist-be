{
  "name": "captionist-backend",
  "version": "1.0.0",
  "description": "Backend API for Captionist video captioning application",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t captionist-backend .",
    "docker:run": "docker run -p 3001:3001 captionist-backend"
  },
  "keywords": [
    "video",
    "captions",
    "transcription",
    "ffmpeg",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
